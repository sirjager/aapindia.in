<!-- This needs to be inlinded to avoid FOUC (Flash of unstyled content) -->
<script is:inline lang="js">
  const __apply_theme__ = () => {
    const _s = localStorage.getItem("theme");
    if (typeof localStorage !== "undefined" && _s)
      document.documentElement.setAttribute("data-theme", _s);
    else document.documentElement.setAttribute("data-theme", "light");
  };
  __apply_theme__();
</script>
